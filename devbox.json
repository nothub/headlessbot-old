{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.12.0/.schema/devbox.schema.json",
  "packages": [
    "binutils@2.43.1",
    "curl@8.10.1",
    "docker@27.3.1",
    "jq@1.7.1",
    "temurin-bin-21@latest"
  ],
  "shell": {
    "scripts": {
      "bot": [
        "./scripts/bot.sh"
      ],
      "server": [
        "./scripts/server.sh"
      ],
      "monitoring": [
        "./scripts/monitoring.sh"
      ],
      "test": [
        "./gradlew --console=plain --info --full-stacktrace check"
      ]
    }
  }
}
